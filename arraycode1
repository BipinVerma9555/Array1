#include<stdio.h>
int main()
{
    int a[100],num,N,i,c;
    printf("input the number of element");
    scanf("%d",&N);
    printf("input the list");
    for(i=0;i<N;i++){
    scanf("%d",&a[i]);}
    printf("input the number you want search");
    scanf("%d",&num);
    c=0;
    for(i=0;i<N;i++)
    {
        if(a[i]==num)
        c++;
    }
